                    d="M11,4.2h1.1l0.2,0.1c0,0,0.2,2.8,0.2,3.7c0,0.9-0.1,4.5-0.1,4.5l-0.3,0.1h-1l-0.3-0.1  c0,0-0.2-2.9-0.2-4.4s0.2-3.7,0.2-3.7L11,4.2z"
                  />
                  <path
                    d="M4.3,4.2h1l0.2,0.1c0,0,0.2,2.8,0.2,3.7c0,0.9-0.1,4.5-0.1,4.5l-0.3,0.1H4.4l-0.3-0.1  c0,0-0.2-2.9-0.2-4.4s0.2-3.7,0.2-3.7L4.3,4.2z"
                  />
                  <path
                    d="M7.7,4.2h1L9,4.2c0,0,0.2,2.8,0.2,3.7c0,0.9-0.1,4.5-0.1,4.5l-0.3,0.1H7.8l-0.3-0.1  c0,0-0.2-2.9-0.2-4.4s0.2-3.7,0.2-3.7L7.7,4.2z"
                  />
                  <g>
                    <rect x="0.4" y="13.1" width="12.3" height="0.6" />
                    <rect y="14.3" width="13.1" height="0.7" />
                  </g>
                </svg>
              </a>
            </li>
          <% } %>
        </ul>
        <% if (htmlWebpackPlugin.options.copyright) { %>}
          <p>&copy;<%= new Date().getFullYear() %> <%= htmlWebpackPlugin.options.copyright %> | All Rights Reserved</p>
        <% } %>
        <button id="show_hidden_panel" class="mui-btn" aria-label="Show hidden panel"></button>
      </footer>
    </div>
    <template id="more-dropdown--template">
      <div class="mui-dropdown">
        <button class="mui-btn" data-mui-toggle="dropdown" title="More">
          <svg class="w-24" viewBox="0 0 24 24">
            <path
              d="M12,16A2,2 0 0,1 14,18A2,2 0 0,1 12,20A2,2 0 0,1 10,18A2,2 0 0,1 12,16M12,10A2,2 0 0,1 14,12A2,2 0 0,1 12,14A2,2 0 0,1 10,12A2,2 0 0,1 12,10M12,4A2,2 0 0,1 14,6A2,2 0 0,1 12,8A2,2 0 0,1 10,6A2,2 0 0,1 12,4Z"
            />
          </svg>
        </button>
        <ul class="mui-dropdown__menu mui-dropdown__menu--right">
          <li>
            <a class="more-cover" href="" target="_blank">
              <img src="" loading="lazy" alt="Album cover" />
            </a>
            <div class="mui-divider"></div>
          </li>
          <li>
            <a class="more-link" href="" target="_blank">
              <svg viewBox="0 0 24 24" aria-hidden="true">
                <path d="M5,20H19V18H5M19,9H15V3H9V9H5L12,16L19,9Z" />
              </svg>
              Download Zip
            </a>
          </li>
        </ul>
      </div>
    </template>
    <% htmlWebpackPlugin.files.js.forEach(js => { %>
      <script type="text/javascript" src="<%= js %>"></script>
    <% }) %>
  </body>
</html>
